{
  "name": "promptvexity",
  "version": "0.1.0",
  "private": true,
  "author": "jjtheshooterr",
  "repository": {
    "type": "git",
    "url": "https://github.com/jjtheshooterr/promptvexity.git"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"{app,components,lib}/**/*.{js,jsx,ts,tsx}\"",
    "seed": "tsx scripts/seed.ts",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:coverage:open": "vitest --coverage && start coverage/index.html",
    "test:actions": "vitest --run actions",
    "test:components": "vitest --run components",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:run && npm run test:e2e",
    "test:verify": "npm run test:run && echo All tests passing!"
  },
  "dependencies": {
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.39.0",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@vercel/analytics": "^1.6.1",
    "autoprefixer": "^10.4.23",
    "csv-parse": "^5.5.3",
    "dotenv": "^17.2.3",
    "next": "16.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sonner": "^2.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "eslint": "^8",
    "eslint-config-next": "15.5.9",
    "jsdom": "^27.4.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.0",
    "typescript": "^5",
    "vitest": "^4.0.18"
  }
}